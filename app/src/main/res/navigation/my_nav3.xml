<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav3"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.etpproject.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_categorySpecificFragment"
            app:destination="@id/categorySpecificFragment" />
        <action
            android:id="@+id/action_homeFragment_to_bookDetailsFragment2"
            app:destination="@id/bookDetailsFragment2" />
    </fragment>
    <fragment
        android:id="@+id/categorySpecificFragment"
        android:name="com.example.etpproject.categorySpecificFragment"
        android:label="fragment_category_specific"
        tools:layout="@layout/fragment_category_specific" >
        <action
            android:id="@+id/action_categorySpecificFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <argument
            android:name="catname"
            app:argType="string" />
        <action
            android:id="@+id/action_categorySpecificFragment_to_bookDetailsFragment2"
            app:destination="@id/bookDetailsFragment2" />
    </fragment>
    <fragment
        android:id="@+id/bookDetailsFragment2"
        android:name="com.example.etpproject.BookDetailsFragment"
        android:label="fragment_book_details"
        tools:layout="@layout/fragment_book_details" >
        <action
            android:id="@+id/action_bookDetailsFragment2_to_categorySpecificFragment"
            app:destination="@id/categorySpecificFragment" />
        <action
            android:id="@+id/action_bookDetailsFragment2_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>